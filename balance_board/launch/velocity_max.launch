<launch>
    <include file="$(find panda_moveit_config)/launch/panda_control_moveit_rviz.launch">
        <!-- <arg name="robot_ip" value="robot.franka.de"/> -->
        <arg name="launch_franka_control" value="false"/>
    </include>

    <!-- <node name="realsense_node" pkg="balance_board" type="CV_test"/> -->
    <node name="joint_controller_interface" pkg="balance_board" type="joint_controller_interface">
        <rosparam command="load" file="$(find balance_board)/config/interface_params.yaml"/>
    </node>

    <node name="velocity_max_test" pkg="balance_board" type="velocity_max_test">
        <rosparam command="load" file="$(find balance_board)/config/dtc_params.yaml"/>
    </node>

    <node name="plotter" pkg="balance_board" type="plot_node"/>

</launch>